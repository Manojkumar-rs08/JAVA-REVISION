package string;

public class Stringparanthesis {

	public static void main(String[] args) {
		
		int count = 0;
		
		String s = ")()";
		
		String[] s1 =s.split("");
		
		for(int i =0; i<s1.length;i++)
		{
			
			
			if(s1[i].equals("(")){
			
				count++;
			if(s1[i].equals(")"))
			{
		count++;
			}
			}

	}
		System.out.println(count);
}
}