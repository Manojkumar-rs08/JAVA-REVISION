package string;

public class Stringparanthesis {

	public static void main(String[] args) {
		
		
		int count=0;
	String s1=")()";
	
	String[] s2 = s1.split("");
	
	System.out.println(s2.length);
	
			
			for(int i=0;i<s2.length;i++) {
				
				if((s2[i].charAt(i)=='(')&&(s2[i].charAt(i+1)==')')) {
					
					
					count++;
				}
				
			}
	
			
	}			
			
	
	
		
}