package array;

public class Arrayascending { 

	public static void main(String[] args) {
		 int a[]= {5,8,2,0,0,0,45,20,1,10,};
	       
	       int aes=0;
			
			for (int i=0;i<=a.length-1;i++)
			{
				for (int j=i+1;j<=a.length-1;j++)
				{
					if(a[i]<a[j])
					{
						aes=a[j]; 
						a[j]=a[i];
						a[i]=aes;	
					}
		
					
				}
					System.out.println(a[i]);
	
				
					
			}		
	}

}
