package string;

public class Stringparanthesis {

	public static void main(String[] args) {
		
		
		int count =0;
		String s = ")()";
		
		String[] s1 =s.split("");
		
	for(int i=0; i<=s1.length-1;i++) {
		
		
		if(s1[i].charAt(i)=='(')
		{
			count++;
			
		}
		
	}
		
			
			
	System.out.println(count);		
			
	}			
			
	
	
		
}