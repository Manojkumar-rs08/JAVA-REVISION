package string;

public class Stringparanthesis {

	public static void main(String[] args) {
		
		int count = 0;
		
		String s = ")()";
		
		String[] s1 =s.split("");
		
		for(int i =0; i<s1.length;i++)
		{
			String s2 = "()";
			
			if(s1[i].contains("(")){
				
				count++;
			if(s1[i].contains(")"))
			{
			count++;
			}
			}

	}
		System.out.println(count);
}
}