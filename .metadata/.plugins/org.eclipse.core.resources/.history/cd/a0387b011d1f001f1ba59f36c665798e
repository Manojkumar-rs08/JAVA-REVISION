package array;


public class Aryindex {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		
		int a[][]= {{3,1,2},{1,7,6},{2,7,7}};
				
		int count=0;
		
		
			for(int i=0;i<3;i++) {
				for(int j=0;j<3	;j++)
				{
				
					if(a[i]==a[j]){
						
	
					System.out.print(a[i][j]+" ");
                    	
				}  
				
	
			}
			

			}	
		 
	}

}
