package string;

public class Stringparanthesis {

	public static void main(String[] args) {
		
		int count = 1;
		
		String s = ")()";
		
		String[] s1 =s.split("");
		
		for(int i =0; i<s1.length;i++)
		{
			String s2 = "()";
			
			if(s1[i].equals(s2)) {
				
				count++;
			}
			
		}
		
System.out.println(count);
	}

}
